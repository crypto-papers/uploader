{
  "name": "cryptopapers_uploader",
  "version": "0.1.0",
  "description": "Desktop app for uploading files to the Cryptopapers database",
  "main": "main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --env main --config ./config/webpack.config.js --mode production",
    "build:renderer": "webpack --env renderer --config ./config/webpack.config.js --mode production",
    "dev": "npm run dev:main & npm run dev:renderer",
    "dev:main": "webpack --watch --env main --config ./config/webpack.config.js --mode development",
    "dev:renderer": "webpack --watch --env renderer --config ./config/webpack.config.js --mode development",
    "start": "electron dist/main.js",
    "test": "jest --config config/jest.config.js"
  },
  "author": "Marek Rewers",
  "license": "MIT",
  "prettier": "@cryptopapers/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@cryptopapers/eslint-config": "^0.1.0",
    "@cryptopapers/prettier-config": "^1.0.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/jest-axe": "^3.2.2",
    "@types/react": "^16.9.19",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.3.0",
    "jest-axe": "^3.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "electron": "^8.5.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-feather": "^2.0.3"
  }
}
